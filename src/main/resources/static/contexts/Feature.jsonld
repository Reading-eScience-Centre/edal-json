{
  "@context": {
    "type": "@type",
    "id": "@id",
    "dct": "http://purl.org/dc/terms/",
    "w3time": "http://www.w3.org/2006/time#",
    "oml": "http://def.seegrid.csiro.au/ontology/om/om-lite#",
    "mel": "http://melodiesproject.eu/ont/geo#",

	"Feature": "oml:Observation",
    "title": "dct:title" ,
    "phenomenonTime": "oml:phenomenonTime",
    "Instant": "w3time:Instant",
    "dateTime": { "@id": "w3time:inXSDDateTime", "@type": "xsd:dateTime" },
    "Interval": "w3time:ProperInterval",
    "start": "w3time:hasBeginning",
    "end": "w3time:hasEnd",
    "observedProperty": { "@id": "oml:observedProperty", "@type": "@id" },
    "featureOfInterest": "oml:featureOfInterest",
    "result": "oml:result",
    
    "RegularGrid": "mel:RegularGrid",
    "Parameter": "mel:Parameter",
    "parameters": "mel:parameter"
  }
}